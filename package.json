{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@samkirkland/ftp-deploy": "^1.1.1",
    "@styled-jsx/plugin-sass": "^4.0.2",
    "dotenv": "^16.0.0",
    "gatsby": "^4.1.3",
    "gatsby-plugin-gatsby-cloud": "^4.1.1",
    "gatsby-plugin-image": "^2.1.2",
    "gatsby-plugin-manifest": "^4.1.3",
    "gatsby-plugin-offline": "^5.1.2",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-root-import": "^2.0.8",
    "gatsby-plugin-sass": "^5.1.1",
    "gatsby-plugin-sharp": "^4.1.3",
    "gatsby-plugin-styled-jsx": "^5.2.0",
    "gatsby-source-filesystem": "^4.1.2",
    "gatsby-transformer-json": "^4.1.0",
    "gatsby-transformer-sharp": "^4.1.0",
    "node-sass": "^6.0.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.43.5",
    "serve": "^13.0.2",
    "styled-jsx": "^4.0.1",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "swiper": "^7.3.1"
  },
  "devDependencies": {
    "@svgr/cli": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "svg:icon:delete": "rimraf ./src/data/icon-svg-react",
    "svg:icon:icon": "svgr src/asset/icon-svg --out-dir ./src/data/icon-svg-react",
    "svgr": "run-s \"svg:icon:*\"",
    "upload": "node deploy.mjs",
    "deploy": "run-s  clean build upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "packageManager": "yarn@3.2.0"
}
